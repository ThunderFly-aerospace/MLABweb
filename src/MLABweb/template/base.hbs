    <script src='/static/tinymce-dist/tinymce.min.js'></script>
    <script src="/static/unveil/jquery.unveil.js"></script>

<!--     <script src="https://cdnjs.cloudflare.com/ajax/libs/ekko-lightbox/5.1.1/ekko-lightbox.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ekko-lightbox/5.1.1/ekko-lightbox.css" /> -->
    <script src="/static/lightbox/dist/ekko-lightbox.min.js"></script>
    <link rel="stylesheet" href="/static/lightbox/dist//ekko-lightbox.min.css" />

    <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.4/css/select2.min.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.4/js/select2.min.js"></script>


    <link rel="stylesheet" type="text/css" href="/static/mlab_style.css">
    
    <script type="text/javascript">
      $(document).ready(function() {
        $("img").unveil(2000);
      });

      $("<img/>")
          .on('load', function() { console.log("image loaded correctly"); })
          .on('error', function() { console.log("error loading image"); })
          .attr("src", $("originalImage").attr("src"));

      $("img").unveil(200, function() {
        $(this).load(function() {
          this.style.opacity = 1;
        });
      });

      function setLang(lang){
        console.log("set lang", lang);
        document.cookie = "locale="+lang+";path=/";
        location.reload();
      }


  </script>

    
  {% block head %}{% end %}
</head>
<body >
  <!--{% include 'nav.hbs' %}-->

  <div class="header" style="position: fixed;">
  <div class="container">
    <a href="/"><img src="/static/logo.png" height="35pt" alt="MLAB"></img></a>
    <ul class="nav nav-pills pull-right">
      <li class="active"><a href="/">{{ _("Home")}}</a></li>

      <li class=""><a href="/#onas">{{ _("About")}}</a></li>
      <li class=""><a href="/modules?status=2">{{ _("Modules")}}</a>
      <li class=""><a href="#Cooperation">{{ _("Cooperation")}}</a></li>
      <li class=""><a href="/#home_contact">{{ _("Contact")}}</a></li>
      <li class=""><a href="https://github.com/MLAB-project">GitHub</a></li>
      <li class=""><a href="http://wiki.mlab.cz/">Wiki</a></li>
      <li class="submenu"> <a>{{_("Language")}}</a><div class="submenu_show"><ul>
        <li> <a onclick="setLang('cs_CZ')"><img src="/static/media/flags/CZ.png"> Cs </a></li>
        <li> <a onclick="setLang('en_UK')"><img src="/static/media/flags/GB.png"> En </a></li>
      </ul></div></li>
      <li>{%if current_user %} <a href="/logout/">{{ _("Log out")}} <small> {{current_user['civil_name']}}</small> {%else%} <a href="/login/"> {{ _("Log in") }} {%end%}</a></li>
    </ul>
    
  </div><hr style="margin: 0px; margin-top: 10pt;">
  </div>
<div id="wrapper">


      <hr>
  
  {% block container %}{% end %}

  <div class="container" style="min-width: 90%; max-width: 100%;">
  <br>
      <div class="container-fluid">
          <div class="row">
              <div class="col-lg-12">
                <div class="container" style="min-width: 90%; max-width: 100%;">
                    {% block content %}{% end %}
                </div>
              </div>
          </div>
      </div>
  </div>

  <div>
  {% include 'footer.hbs' %}
  </div>
   


</div></body>

